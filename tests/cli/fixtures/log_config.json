{
  "version": 1,
  "formatters": {
    "detailed": {
      "format": "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "detailed",
      "stream": "ext://sys.stdout"
    }
  },
  "loggers": {
    "faststream.access.redis": {
      "level": "DEBUG",
      "propagate": false
    },
    "faststream": {
      "level": "DEBUG",
      "handlers": ["console"],
      "propagate": false
    }
  }
}
